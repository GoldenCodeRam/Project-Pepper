{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "require-jsdoc": "warn",
      "max-len": "off",
      "no-unused-vars": "warn",
      "no-trailing-spaces": "off",
      "indent": "off"
    }
}
